    "require": {
        "php": ">=7.2",
        "twilio/sdk": "~6.0",
        "illuminate/notifications": "^5.8 || ^6.0 || ^7.0 || ^8.0",
        "illuminate/support": "^5.8 || ^6.0 || ^7.0 || ^8.0",
        "illuminate/events": "^5.8 || ^6.0 || ^7.0 || ^8.0",
        "illuminate/queue": "^5.8 || ^6.0 || ^7.0 || ^8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Twilio\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\Twilio\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "NotificationChannels\\Twilio\\TwilioProvider"
            ]
        }
    }
}
